<resources>
    <integer name="def_screen_off_timeout">120000</integer>
    <bool name="def_accelerometer_rotation">false</bool>
    <!-- Comma-separated list of location providers.
         Network location is off by default because it requires
         user opt-in via Setup Wizard or Settings.
    -->
    <string name="def_location_providers_allowed" translatable="false"></string>
    <bool name="def_lockscreen_disabled">true</bool>
    <!-- Default for Settings.Secure.DEFAULT_INPUT_METHOD - NV's gamepad keyboard -->
    <add-resource type="string" name="def_default_input_method" />
    <string name="def_default_input_method" translatable="false">com.android.inputmethod.latin/.LatinIME</string>
    <!-- Default for Settings.Secure.BACKUP_TRANSPORT - Google cloud backup -->
    <string name="def_backup_transport" translatable="false">com.google.android.backup/.BackupTransportService</string>
</resources>
